<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignUpStep3Activity">

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/SignUpPhoneNumberInput"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="휴대폰 번호"
        android:inputType="text"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/nextButton3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/round_shape_green"
        android:text="다음"
        app:layout_constraintTop_toBottomOf="@id/SignUpPhoneNumberInput" />
</androidx.constraintlayout.widget.ConstraintLayout>
